{
  "name": "@ox2/ui",
  "version": "0.0.2",
  "description": "UI component library.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ox2/ui"
  },
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook -p 60066 -s ./public",
    "build-storybook": "start-storybook -p 60066 -s ./public",
    "test-storybook": "storyshots",
    "build": "npm run build:babel && npm run build:copy-files",
    "build:babel": "babel ./src --ignore *.story.js --out-dir ./build",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "prebuild": "npm run clean:build",
    "lint": "eslint src test/integration && echo \"eslint: no lint errors\"; exit 0",
    "lint:find-rules": "eslint-find-rules -u .eslintrc.js",
    "test": "cross-env NODE_ENV=test babel-node test/index.js",
    "test:coverage": "cross-env NODE_ENV=test babel-node ./node_modules/istanbul/lib/cli.js cover test/index.js",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "test:integration": "cross-env NODE_ENV=test babel-node test/index.js integration",
    "test:integration:watch": "cross-env NODE_ENV=test babel-node test/watch.js integration",
    "test:unit": "cross-env NODE_ENV=test babel-node test/index.js unit",
    "test:unit:watch": "cross-env NODE_ENV=test babel-node test/watch.js unit",
    "pretty:data": "find ./data -name \"*.json\" | xargs -Irepl js-beautify repl -r"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.31.0",
    "@kadira/storybook-addon-options": "^1.0.1",
    "@kadira/storyshots": "^2.1.0",
    "@ox2/button-css": "^1.0.0",
    "@ox2/css-font-roboto": "^1.0.0",
    "@ox2/css-font-roboto-condensed": "^1.0.0",
    "@ox2/theming": "^1.0.0",
    "@ox2/ycss": "^1.0.3",
    "@tapfuse/css-icons-linear": "^1.0.0",
    "@tapfuse/theme-spark": "^1.0.0",
    "app-module-path": "^2.1.0",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.11.1",
    "eslint-find-rules": "^1.11.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-mocha": "^4.0.0",
    "eslint-plugin-react": "^6.7.1",
    "fs-extra": "^1.0.0",
    "git-url-parse": "^6.0.1",
    "glob": "^7.0.5",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.9.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-addons-perf": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.3",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1"
  },
  "main": "./index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "dependencies": {
    "@ox2/button": "^1.0.0",
    "@ox2/icon": "^1.0.0",
    "@ox2/image": "^0.0.1",
    "classnames": "^2.2.5"
  }
}
